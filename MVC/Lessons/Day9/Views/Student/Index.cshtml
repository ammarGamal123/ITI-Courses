@model List<Student>
@{
    ViewData["Title"] = "Index";
}

<a asp-controller="Student" asp-action="New" 
class="btn btn-success">
    Add New Student With Tag Helper
</a>

<br />
<h1>Index</h1>

<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th>Student Id</th>
            <th>Student Name</th>
            <th>Student Address</th>
            <th>Student Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Address</td>
                <td>@item.Age</td>
            </tr>
        }
    </tbody>
</table>

