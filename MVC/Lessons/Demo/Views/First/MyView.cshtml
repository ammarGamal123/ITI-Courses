<!--Razor Engine Server Side-->
<!--Block Of Code is the first thing implemented while rendering-->

@{
    int x = 13;
    int y = 12;

    int product = x * y;
}

<!--Inline Code-->
<h1>@product</h1>


<!--Condition Statement-->
@if (x == y)
{
    <h1 style="color:brown">X is equal Y</h1>
}else if (x != y){
    <h1 style="color:chartreuse">X is Not Equal Y</h1>
}

<!--Looping-->

<ul>
@for(int i = 0;i < 10; i++)
{
    <li>@i</li>   
}
</ul>